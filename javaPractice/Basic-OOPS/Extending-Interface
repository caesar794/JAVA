interface area
{	public void ar(int num1,int num2);
}
interface arr extends area
{	public void are(int side);
}
class ext implements arr
{	public int len,bre,side,res;
	public void ar(int len,int bre)
	{	res=(len*bre);
		System.out.println("Area (length,breadth) : "+res);
	}
	public void are(int side)
	{	res=(side*side);
		System.out.println("Area (side) :"+res );
	}
}
class extint
{	public static void main(String args[])
	{	ext obj=new ext();
		obj.ar(10,5);
		obj.are(5);	
	}
}
